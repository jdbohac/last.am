
  <div >
      
      <table class="table is-hoverable is-fullwidth" style="background-color: rgb(245, 203, 203);margin-top:3rem; border-radius: 10px;box-shadow:4px 4px .3rem; color:grey;">
      <%for (let i=data.length -1;i>=0;i--){%>
      <tr class="is-clickable">
        <div>
          <td style="width:10rem;" valign="middle" align="left">
            <a href="/last.am/show/<%=data[i]._id%>">
              <div class="my-1">
                <img class="image is-128x128" src=" <%=data[i].gameScreen%>" style="border-radius: 4px;">
              </div>
            </a>
          </td>
          <td class="is-vcentered has-text-centered">
            <a href="/last.am/show/<%=data[i]._id%>">
              <%=data[i].gameName%>
            </a>
          </td>
          <td class="is-vcentered">
            <a href="/last.am/show/<%=data[i]._id%>">
              <%=data[i].pub%>
            </a>
          </td>
          <td class="is-vcentered">
            <a href="/last.am/show/<%=data[i]._id%>">
              <%=data[i].year%>
            </a>
          </td>
        </div>
      </tr>

      <%}%>
    </table>
    </div>